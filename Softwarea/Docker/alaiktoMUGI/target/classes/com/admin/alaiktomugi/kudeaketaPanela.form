              <AuxValues>
                <AuxValue name="JavaCodeGenerator_AddingCodePost" type="java.lang.String" value="public void cargarHistorikoa() {&#xa;    String[] columnas = { &quot;Amaiera data&quot;, &quot;Amaiera ordua&quot;, &quot;Jatorria&quot;, &quot;Helmuga&quot;, &quot;Bidaia ID&quot; };&#xa;    DefaultTableModel model = new DefaultTableModel(null, columnas);&#xa;&#xa;    try (Connection conn = Konexioa.getConnection()) {&#xa;        String sql = &quot;SELECT amaiera_data, amaiera_ordua, jatorria, helmuga, bidaia_id_bidaia FROM historikoa&quot;;&#xa;        PreparedStatement stmt = conn.prepareStatement(sql);&#xa;        ResultSet rs = stmt.executeQuery();&#xa;&#xa;        while (rs.next()) {&#xa;            Object[] fila = new Object[5];&#xa;            fila[0] = rs.getDate(&quot;amaiera_data&quot;);&#xa;            fila[1] = rs.getTime(&quot;amaiera_ordua&quot;);&#xa;            fila[2] = rs.getString(&quot;jatorria&quot;);&#xa;            fila[3] = rs.getString(&quot;helmuga&quot;);&#xa;            fila[4] = rs.getInt(&quot;bidaia_id_bidaia&quot;);&#xa;            model.addRow(fila);&#xa;        }&#xa;&#xa;        HistorikoaTable.setModel(model); // asignar datos a la tabla&#xa;&#xa;    } catch (SQLException e) {&#xa;        e.printStackTrace();&#xa;        JOptionPane.showMessageDialog(null, &quot;Errorea datuak kargatzean: &quot; + e.getMessage());&#xa;    }&#xa;}"/>
              </AuxValues>
            </Component>
